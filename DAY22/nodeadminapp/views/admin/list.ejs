    <!--우측 상단 헤더 영역 -->
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-lg-10">
            <h2>관리자계정 관리</h2>
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    <a href="/">홈</a>
                </li>
                <li class="breadcrumb-item">
                    <a>관리자계정</a>
                </li>
                <li class="breadcrumb-item active">
                    <strong>목록</strong>
                </li>
            </ol>
        </div>
    </div>

    <!--우측 콘텐츠 영역 -->
    <div class="wrapper wrapper-content animated fadeInRight ecommerce">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox">
                    <div class="ibox-content">
                        <a href="/admin/create" style="display: flex; justify-content: flex-end; margin-bottom: 1rem;"><button class="btn-white btn btn-xs">관리자 신규등록</button></a>
                        <table class="footable table table-stripped toggle-arrow-tiny" data-page-size="15">
                            <thead>
                                <tr>
                                    <th>Order ID</th>
                                    <th name="name">관리자명</th>
                                    <th name="email">이메일</th>
                                    <th name="telephone">전화번호</th>
                                    <th name="dept_name">부서이름</th>
                                    <th name="reg_date">등록일시</th>
                                    <th class="text-center">설정</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for(var i=0; i<admins.length;i++){ %>
                                <tr>
                                    <td><%=admins[i].admin_member_id%></td>
                                    <td><%=admins[i].admin_name%></td>
                                    <td><%=admins[i].email%></td>
                                    <td><%=admins[i].telephone%></td>
                                    <td><%=admins[i].dept_name%></td>
                                    <td><%=admins[i].reg_date%></td>
                                    <td class="text-center">
                                        <div class="btn-group">
                                            <a href="/admin/modify"><button class="btn-white btn btn-xs">수정</button></a>
                                            <!-- <a href="/admin/delete"><button class="btn-white btn btn-xs">삭제</button></a> -->
                                        </div>
                                    </td>
                                </tr>
                                <% } %>
                            </tbody>
                            <tfoot>
                            <tr>
                                <td colspan="7">
                                    <ul class="pagination float-right"></ul>
                                </td>
                            </tr>
                            </tfoot>
                        </table>

                    </div>
                </div>
            </div>
        </div>

    </div>

